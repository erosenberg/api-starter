    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bulma": "^0.6.0",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.5",
    "ejs": "^2.5.7",
    "env-cmd": "^7.0.0",
